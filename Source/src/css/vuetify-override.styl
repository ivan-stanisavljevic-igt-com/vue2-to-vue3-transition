@import "fonts";
@import "config";

.url-state-name-event
  .url-sports
  .url-event
  .v-tabs
  .v-tabs__bar
  .v-tabs__wrapper
   @media mobile-big-and-below
    background #fff
.v-tabs__bar
  .v-icon.material-icons
    display none !important
.application
  font-family: 'Open Sans Regular'

  a
    color: color-blue
    text-decoration: none

  .primary
    background-color: #9E864D !important;
    border-color: #9E864D !important;

  .secondary
    background-color: #8A4659 !important;
    border-color: #8A4659 !important;
.xtremepush_menu
  overflow: visible !important
  contain: none !important
  &:before
    material-icons()
    display: block
    font-size 35px
    content: 'arrow_drop_up'
    margin: auto
    color: #fff
    position: absolute
    top: -20px
    right: 15px
    margin-left: 0
    display: block
    z-index: 1000
.v-menu__activator
  #xtremepushInboxBtn
    background: transparent !important
.menu_with_top_arrow
  background: transparent !important
  overflow: visible !important
  contain: none !important
  margin-left: 133px !important
  margin-top: 10px !important
  position: fixed
  top: 66px !important

  &:before
    material-icons()
    display: block
    content: 'arrow_drop_up'
    margin: auto
    color: #20385A
    position: absolute
    top: -14px
    left: 50%
    margin-left: -5px

.menu_top_arrow_right
  background: transparent !important
  overflow: visible !important
  contain: none !important
  margin-top: 5px !important
  position: absolute
  margin-left: -25px !important
  @media mobile-big-and-below
    margin-left: 5px !important
  &:before
    material-icons()
    display: block
    content: 'arrow_drop_up'
    margin: auto
    color: #fff
    position: absolute
    top: -14px
    left: 90%
    margin-left: -5px

.account-info
  .v-menu__activator
    button
      background: #FFFFFF !important
      color: #1493FF
      min-width: 40px !important
      &:hover
        // background: #31568F !important
        opacity: 0.8

      .v-btn__content
        padding: 0 8px

    .igt-icon
      padding-right : 0!important

    #xtremepushInboxBtn:not(.btn--depressed):focus
      .btn__content
       position static
    @media mobile-big-and-below
      #xtremepushInboxBtn:not(.btn--depressed):hover
        opacity 1
    @media mobile-small-and-below
      #xtremepushInboxBtn
        margin 6px 4px 6px 0px

.v-menu__content
  background: #fff
  z-index: 50 !important
.url-my-events-settings
.url-joinUs
.url-verify-personal-data
  .v-menu__content.select_list
    margin-top 40px  !important

.v-menu.with-arrow
  .v-menu__activator
    button
      background-color: general-color-background !important
      color: general-color-font !important
      padding: 0 10px 0 20px

      .v-btn__content
        flex-shrink: 1

      &:after
        material-icons()
        display: block
        content: 'arrow_drop_down'
        margin-left: 5px

.list
  margin: 0
.application
  .v-tabs-wrapper
    margin-bottom: 0
  .container.headlines
    .headline
      .title
        font-size: 30px!important
        @media mobile-big-and-below
          font-size: 20px!important
      .event
        margin: 0
      .circular-rotate-container
        .progress-circular.white--text
          margin: 0
        .progress-circular__overlay
          transition: all 1s linear
        .progress-circular__underlay
          stroke: rgba(0,0,0,0)
    .arrows-container
      position: absolute
      bottom: -55px
      right: 0

    .v-tabs
      background: unset
      width: 90%
      .v-tabs__bar
        background: transparent
        @media tablet-and-above
          overflow: hidden
          height: 85px
          position: relative
          left: -20px
        .v-tabs__container
          display inline-block
          white-space nowrap
          height: 85px
          @media tablet-and-above
            overflow: auto
            white-space: nowrap
            height: 120px
            position: relative
            left: 20px
          @media tablet
            display inline-block
            white-space nowrap
            overflow auto
            height 200px
            width 600px
            transform: none!important // turned off auto translatonX
            padding 0 17px 0 0
          .v-tabs__div.tab
            display inline-block
            width 28%
            max-width unset
            border none
            margin: 0 20px 15px
            border-radius: 5px
            @media tablet-and-above
              height 85px
            @media tablet
              display inline-block
              margin 0 10px 15px
              width 47%
              height 84px
            @media only screen and (min-width: 1190px) and (max-width: 1240px)
              width 48%
            .ripple__container
              display: none
            a
              color:#ffffff!important
              opacity: initial
              padding: 16px 0px
              display: block
              span
                display: block
                text-transform: initial
            &.active
              border:none
  .v-tabs__div
    min-width unset
    max-width unset
  .v-tabs
    .v-tabs__bar
      background none
      @media mobile-big-and-below
        // background: #06283b

      @media mobile-big-and-below
        // background: rgba(35, 150, 252, 0.4)
      .icon.v-tabs__icon
        display: none
      .v-tabs__wrapper
        border-top-left-radius: 0px
        border-top-right-radius: 0px

        @media mobile-big-and-below
          border-radius: 0
      .v-tabs__wrapper--show-arrows
        margin-left: 0
        margin-right: 0
      .v-tabs__container
        padding-left: 0
        height: 56px
        @media mobile-big-and-below
          height: 44px
        .v-tabs__div
          text-transform: none
          flex 1
          @media mobile-big-and-above
            min-width unset
            max-width unset
          a
            color: #0b4da1
            font-size: 14px
            font-family: 'Open Sans Bold'
            opacity: 1
            padding: 6px
            text-transform uppercase

            @media mobile-big-and-below
              font-size: 14px
              font-weight: normal

            *
              display: none
            span
              display block !important
          &.tab
            border: none
            margin: 0
            background #fff
            @media mobile-big-and-below
              background #fff
          &.tab.active
            background: #0b4da1
            color #fff

            @media mobile-big-and-below
              background: #0b4da1
              border-top-left-radius: 0px
              border-top-right-radius: 0px

              &:nth-child(2)
                border-top-left-radius: 0
              &:last-child
                border-top-right-radius: 0


            a
              color: #fff
              font-family 'Open Sans Bold'
            a .badge
              font-weight: normal

        .v-tabs__slider-wrapper
          display: none
        .v-menu.v-tabs__div
          background #fff

  label
    font-size: 14px

  &.checkbox
    &.label-to-left
      label
        position: static
        width: auto
        margin-right: 5px

      .v-input-group__input
        flex-basis: 0%

.url-live
  .v-tabs__container
    height 56px !important

.picker--date
  table
    border-collapse: separate

    td
      padding: 0

  .range-start
  .range-in
  .range-end
    border-radius: 0
    top: 0
    right: 0
    bottom: 0
    left: 0
    content: ""
    display: block
    position: absolute
    -webkit-transform: none
    transform: none
    width: auto
    height: auto
    background-color: rgba(76, 173, 255, 0.15)
    z-index: 1

  .range-start
    left: 50%

  .range-end
    right: 50%

.date-picker-table
  .v-btn
    z-index: 2

    &.v-btn--outline
      background-color: #0d7fe1 !important
      border-color: #0d7fe1 !important

      .v-btn__content
        color: #fff

    &.accent
      background-color: #0d7fe1 !important
      border-color: #0d7fe1 !important
      z-index: 3

.v-btn
  color:black
  text-transform: capitalize
  height: 40px
  &:not(.v-btn--depressed)
    box-shadow: none !important
  .v-btn__content
    &:before
      display: none
      .ripple__container
        display: none

  &.primary-btn
    background-color: #2396FC
  &.primary-btn-hit
    background-color: #DFA32C !important
  &.primary-btn-disable
    background-color: #FAD88E !important
  &.primary-btn-blue.v-btn.v-btn--disabled:not(.v-btn--icon):not(.v-btn--flat)
    background-color: #F5F8FC !important
    border: 1px solid #CFD6DB
    color: #99A8B1 !important
    height: 42px
    font-size: 14px
  &.primary-btn-black.btn.btn--disabled:not(.v-btn--icon):not(.v-btn--flat),
  &.primary-btn-black
    background-color: #111 !important
    color: #fff !important
  &.primary-btn-blue
    background-color: #0b4da1 !important
    color: #fff !important
    height: 42px
    font-size: 14px
  &.primary-btn-blue:hover
    background-color: #157cc1
  &.secondary-btn
    background-color: #D0D0D0
    :hover
      background-color:#ADADAD
  &.secondary-btn-hit
    background-color: #ADADAD !important
  &.secondary-btn-disable
    background-color: #D0D0D0 !important

  &.close
    height: 32px
    color: #fff

    .icon
      font-size: 32px
      font-weight: bold

      @media mobile-big-and-below
        font-size: 26px

.account-info
  .deposit
    .v-btn__content
      flex-wrap: wrap

.login-info
  .v-btn__content
    font-family: 'Open Sans Bold'

.v-dialog
  background: #fff
  border-radius: 0
  box-shadow: none
  padding: gap
.v-dialog.firstVisit
  background-position: center
  background-size: cover
  overflow: hidden

.v-dialog.sightline-transaction-dialog
  padding 0
  border-radius 4px

.v-input-group
  margin-bottom: gap
  max-width: 400px

  .application &
    input
      color: #818E95

  @media mobile-big-and-below
    max-width: 100%

  .v-input-group__input
    background: #fff
    padding: 25px 12px 0
    border-radius: 2px

  &.v-input-group--text-field input
    font-size: 16px

  &.username .input-group__input
    border-radius: 2px 2px 0 0

  &.password .input-group__input
    border-radius: 0 0 2px 2px
    border-top: 0px !important

  &.password:not(.input-group--focused) .input-group__input
    border-top: 0px !important

  &.v-input-group--focused.input-group--text-field .input-group__input
    border: 1px solid #2396FC !important

  &.input-group--text-field .input-group__input
    height: 68px
    border: 1px solid #CFD6DB

  &.input-group--text-field label
    top: 20px
    left: 12px
    text-transform: uppercase
    font-weight: normal
    color: #1f375b !important

  &.input-group--focused.input-group--text-field input
     color: #1f375b !important

   &.input-group--focused.input-group--text-field label
     color: #2396FC !important


  &.input-group .input-group__append-icon
    padding: 0px 12px 15px 10px

  &.input-group--text-field:not(.input-group--single-line).input-group--focused:not(.input-group--textarea) label,
  &.input-group--text-field:not(.input-group--single-line).input-group--placeholder:not(.input-group--textarea) label,
  &.input-group--text-field.input-group--dirty:not(.input-group--textarea) label
    transform: none

  &.input-group.input-group--text-field:not(.input-group--single-line):not(.input-group--error).input-group--focused label
    color: #A6A6A6

  .input-group__details
    min-height: 2px

    &:before,
    &:after
      display none
.v-overlay
  z-index 10
.v-overlay--active:before
    opacity: .90

.v-overlay:before
    background-color: #111C2C

.preference-switch
  .v-input--switch
    flex:  0 auto
    margin-left: auto
    .v-input__slot
      margin 0 !important
    .v-input--selection-controls:not(.v-input--hide-details) .v-input__slot
      margin-bottom 0 !important
    .v-input--selection-controls__input
      width 50px
      color #fff !important
    .v-input--switch__track
      height 28px
      border-radius 25px
      width 50px
    .v-input--switch__track.theme--light.accent--text
      color #3fc369 !important
      opacity 1
      height 28px
      border-radius 25px
      width 50px
    .v-input--switch__thumb.theme--light.accent--text
      color #fff !important
      height 20px
      width 20px
    .v-input--switch__thumb
      top calc(50% - 4px)
    &.v-input--is-dirty .v-input--switch__thumb
      transform translate(26px) !important
    &.v-input--switch:not(.v-input--is-dirty) .v-input--switch__thumb
      left 5px
  .v-input--selection-controls
    margin-top 0 !important
.url-event
  .v-tabs
    .v-tabs__bar
      .v-tabs__container
        .v-tabs__div.tab
          @media mobile-big-and-below
            background #fff
            height 48px
            border-bottom 2px solid #fff
            max-width 100%
          .v-tabs__item
            @media mobile-big-and-below

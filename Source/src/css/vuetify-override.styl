.browser-microsoft-edge
  .application
    .v-tabs
      .v-tabs__bar
        .v-tabs__container
          width 100%

.v-date-picker-table
  height unset
.v-date-picker-table--date
  table
    td
    th
      padding 0

.application
  .primary
      background-color #06283B !important
      border-color #06283B !important

.v-expansion-panel__body.expand-transition-enter-active.expand-transition-enter-to
  transition none !important

.v-input input:not([type='checkbox'])
  min-height 42px
  padding 10px 5px 0 5px
.theme--light.v-text-field>.v-input__control>.v-input__slot:before
  border-color #cfd6db
.v-select__slot
  label
    font-size 14px
    transform scale(1) !important

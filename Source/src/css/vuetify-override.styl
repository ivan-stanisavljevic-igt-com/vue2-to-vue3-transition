              border-bottom 2px solid #0b4da1

.url-live
  .v-tabs
    .v-tabs__bar
      @media mobile-big-and-below
        background unset
      .v-tabs__container
        .v-tabs__div.tab
          @media mobile-big-and-below
            background #fff
            height 56px
            max-width 100%
            border-bottom 0px
          .v-tabs__item
            @media mobile-big-and-below
              margin-top 4px
              color #0b4da1 !important
              font-weight normal
          &.active
            .v-tabs__item
              @media mobile-big-and-below
                color #fff!important
            @media mobile-big-and-below
              border-top-left-radius 0px
              border-top-right-radius 0px
              border-bottom 0px

// disable slide in tabs
.v-tabs__wrapper .v-tabs__container
  flex 1 !important

.slide-x-reverse-transition //dialog transition slide from right option
  &-leave-to, &-enter
    transform: translateX(100%) !important
    opacity: 1 !important

.browser-internet-explorer
  .application
    .headlines
      .v-tabs
        width calc(100% - 120px)!important
        .v-tabs__bar
          .v-tabs__container
            min-width 550px
              @media desktop-and-above
                margin 0 18px 15px
              @media tablet
                width 46%

      background-color #000038
      background-image none
    .content
      background transparent
  .header_background
    background transparent
    background-image: none
    background-repeat: no-repeat

    @media mobile-big-and-below
      .header_holder
        box-shadow 0 0 3px 0 rgba(0,0,0,0.25)

        .header.header
          .top-container
            background: #000038
            display: flex
            align-items: center
            padding: 0 13px
            .account-widget
              margin: 0
              align-self: center
            .hambMenu
              background none
              color transparent !important
              background-image: url('~@/assets/images/brands/mav/hamburger.svg')
              height: 25px
              width: 22px
              background-repeat: no-repeat
  &.scrolled
    .header_background
      background #2E2C5A

  header.header
    min-height 110px
    flex-direction column
    margin auto
    position unset
    @media mobile-big-and-below
      min-height 55px
    .top-container
      min-height 110px
      display: flex
      justify-content: flex-end
      @media mobile-big-and-below
        min-height 55px
      .rg
        font-size 10px
        color #e5e5e5
        position absolute
        top 7px
        display: flex
        align-items: center
        padding-left 15px
        img
          padding 0 5px
          width 35px
        a
          text-decoration: none
      .rg.postLoginAlertExist
        top 42px
    .logo
      margin-top: 21px
      @media mobile-big-and-below
        margin-top: 0
      img
        max-height 120px
        max-width: 162px
    @media mobile-big-and-below

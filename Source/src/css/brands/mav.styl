      background #000
      .icon
        position relative
        margin-left 20px
        font-size 30px
      .top-container
        background #000
        padding 0 5px
        justify-content space-between
        .v-icon
          color #fff
          background #0B4DA1
          padding 3px
      .logo
        padding 0 !important
        img
          max-height 63px
          max-width: 100%
          padding: 6px 8px 6px
    .header-back-btn
      .icon
        margin-left 0 !important
        color #f1f2f2

    .main-navigation
      margin-right 10px
      margin-top: 30px

      .v-content
        background: #e0e1e2
  .sports-page
  .live-sports-page
    @media mobile-big-and-below
      background #e0e1e2 !important
  &.app-body
    background-color #000
    background-repeat no-repeat
    background-position top center
    background-attachment: fixed
    background-size 100% !important

@import "config"
@import "mixins"
@import "mixins/dn-mixins"

dn-images-path = images-path'/images/brands/dn'

.dn
  &,
  .application
    font-family: "Montserrat Regular"

  h1,
  h2,
  h3,
  h4,
  h5,
  h6
    font-family: "Montserrat Bold"

  .header, button
    letter-spacing: 1px
  @media mobile-big-and-below
    &.app-body.url-state-name-sports.url-sports .sports-page .tab-coupons .v-tabs
      background: transparent
  &.scrolled
    .header
      background-color transparent !important
  .application--wrap
    min-height calc(100vh - 280px)

    @media mobile-big-and-below
      min-height calc(100vh - 260px)

      .v-content
        background: #e8e7eb
  .sports-page
  .live-sports-page
    @media mobile-big-and-below
      background #e7e6ea !important
  &.app-body
    background-image url("/static/brand-img/dn/dn-bg.png")
    background-repeat no-repeat
    background-position top center
    background-attachment: fixed
    background-color #202C38
    background-size 100%
    @media mobile-big-and-below
      background-color #202C38
      background-image none
    .content
      background transparent
  .header_background
    background #fff
    background-image: none
    background-repeat: no-repeat

    @media mobile-big-and-below
      .header_holder
        box-shadow 0 0 3px 0 rgba(0,0,0,0.25)

        .header.header
          .top-container
            background: #fff
            display: flex
            align-items: center
            padding: 0 13px
            .account-widget
              margin: 0
              align-self: center
            .hambMenu
              background none
              color transparent !important
              background-image: url('~@/assets/images/brands/dn/hamburger.svg')
              height: 25px
              width: 22px
              background-repeat: no-repeat

    .sub_header
      height 35px
      // background #3B495F
      margin-top -40px
      box-shadow none
      background linear-gradient(90deg, #fff 50%, #3B495F 50%);
      @media mobile-big-and-below
        display none
        box-shadow 0 2px 2px rgba(0, 0, 0, 0.2)
  header.header
    min-height 138px
    flex-direction column
    margin auto
    position unset
    .logo
      margin-top: 20px

      @media mobile-big-and-below
        margin-top: 0
      img
        max-height 120px
        max-width: 162px
    @media mobile-big-and-below
      min-height 50px
      background #000
      .icon
        position relative
        margin-left 20px
        font-size 30px
      .top-container
        background #000
        padding 0 5px
        justify-content space-between
        .v-icon
          color #fff
          background #0B4DA1
          padding 3px
      .logo
        padding 0 !important
        img
          max-height 63px
          padding: 6px 8px 11px
    .header-back-btn
      .icon
        margin-left 0 !important
        color #3b495f

    .main-navigation
      margin-right 10px
      position: absolute
      bottom: 0
      background #3B495F
      text-align: left
      width: 100%

      &:before
        content: ""
        display: block
        position: absolute
        background: #3b495f
        height: 100%
        width: 100px
        left: -18px
        border-top-left-radius: 7px
        transform: skewX(-25deg)
      ul li a
        padding: 18px 35px
        padding-bottom: 11px
        font-size: 13px
        border-bottom 4px solid transparent

      .router-link-active
        border-bottom 4px solid #ebe200
        font-family: 'Montserrat Regular'
  header.header.postLoginAlertExist
    min-height 173px
  .flex.menu.sm12.align-content-end
    margin-left 10px

  .top_nav
  header .account-widget
    align-self: baseline
    margin-top: 10px

  .top_nav
    width 100%
    text-align right
    padding-right 10px
    white-space: nowrap
    .site-navigation
      .navigation
        .navigation-item
          display inline
          margin 0 5px
        a
          color #000
          text-decoration none !important
          font-family 'Montserrat SemiBold'
          font-size 14px

  .popular
  .bo-navigation

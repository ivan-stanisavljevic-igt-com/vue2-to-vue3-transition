@import "config"
@import "mixins"
@import "mixins/circa-mixins"

.circa
  &,
  .application
    font-family: "Arial"
    --color-accent1: #25e4b8
    --color-accent2: #fd595a
    --font1-bold: 'Tungsten Bold'

  h1,
  h2,
  h3,
  h4,
  h5,
  h6
    font-family: var(--font1-bold)
    font-weight normal
  h1
    font-size 24px
    letter-spacing: 1px
  h2
    font-size 22px
    letter-spacing: 1px
  h4
    font-size 19px
    letter-spacing: 1px
  h5
    font-size 18px
    letter-spacing: 1px
  h3
    font-size 20px !important
    letter-spacing: 1px
  h4
    font-size 19px
    letter-spacing: 1px
  h5
    font-size 18px
    letter-spacing: 1px
  .v-dialog
    .v-card__title.headline
      color #696969
      font-family: var(--font1-bold) !important
      font-size: 25px !important
      letter-spacing: 2px !important
  .header, button
    letter-spacing: 1px
  @media mobile-big-and-below
    &.app-body.url-state-name-sports.url-sports .sports-page .tab-coupons .v-tabs
      background: transparent
  &.scrolled
    .header
      background-color transparent !important
  .application--wrap
    // min-height calc(100vh - 280px)

    @media mobile-big-and-below
      // min-height calc(100vh - 260px)

      .v-content
        background: #000
  .sports-page
  .live-sports-page
    @media mobile-big-and-below
      background #000 !important
  &.app-body
    background-color #000
    .content
      background transparent
  .header_background
    background #000
    background-image: none
    background-repeat: no-repeat

    @media mobile-big-and-below
      .header_holder
        box-shadow 0 0 3px 0 rgba(0,0,0,0.25)

        .header.header
          .top-container
            background: #000
            display: flex
            align-items: center
            padding: 0 13px
            .account-widget
              margin: 0
              align-self: center
            .hambMenu
              background none
              color transparent !important
              background-image: url('~@/assets/images/brands/circa/menu_icon.svg')
              height: 25px
              width: 22px
              background-repeat: no-repeat

    .sub_header
      height 6px
      box-shadow none
      background #171717
      @media mobile-big-and-below
        display none

  &.url-account
    @media mobile-big-and-below
      background #fff !important
      .application--wrap .v-content
        background #fff !important
      .page-content.account
        background #fff !important
      .main-column
        background #fff !important
      .header.header
        .top-container
          background: #0C1F8F !important

  header.header
    min-height 148px
    flex-direction column
    margin auto
    position unset
    .top-container
      min-height 92px
      @media mobile-big-and-below
        min-height 82px
      .rg
        font-size 11px
        color #e5e5e5
        font-family 'Arial'
        position absolute
        top 7px
        display: flex
        align-items: center
        padding-left 15px
        z-index 100
        img
          padding 0 5px
        a
          display flex
          align-items center
          text-decoration: none
          color #e5e5e5
      .rg.postLoginAlertExist
        top 40px
    .top-container.postLoginAlertExist
      margin-top 34px
    .logo
      margin-top: 20px

      @media mobile-big-and-below
        margin-top: 0
      img
        max-height 120px
        max-width: 162px
    @media mobile-big-and-below
      min-height 50px
      background #000
      .icon
        position relative
        margin-left 20px
        font-size 30px
      .top-container
        background #000
        padding 0 5px
        justify-content space-between
        .v-icon
          color #fff
          background #0B4DA1
          padding 3px
      .logo
        padding 0 !important
        img
          max-height 63px
          padding: 6px 8px 11px
          @media only screen and (max-width: 430px)
            max-height: 53px !important
            padding-left: 0px !important
    .header-back-btn
      .icon
        margin-left 0 !important

    .main-navigation
      margin-right 10px
      position: absolute
      bottom: 0
      background #171717
      text-align: left
      width: 100%
      ul li a
        padding: 18px 35px
        padding-bottom: 11px
        font-size: 13px
        border-bottom 4px solid transparent

      .router-link-active
        border-bottom 4px solid var(--color-accent1)
        font-family: 'Arial'
  header.header.postLoginAlertExist
    min-height 214px
  .flex.menu.sm12.align-content-end
    margin-left 10px

  .top_nav header .account-widget
    align-self: baseline
    margin-top: 10px

  .top_nav
    width 100%
    text-align right
    padding-right 10px
    white-space: nowrap
    .site-navigation
      .navigation
        .navigation-item
          display inline
          margin 0 5px
        a
          color #000
          text-decoration none !important
          font-family 'Tungsten Medium'
          font-size 18px

  .popular
  .bo-navigation
    .badgeIconLive
      display none
    .navigation-item
      a
        display flex
        align-items center
        text-transform none
        font-family 'Tungsten Medium'
        font-size 19px !important
        letter-spacing: 1px
        override-svg-icon(icon-names, 'lightgrey')
        span
          display inline-block
        @media mobile-big-and-below
          override-svg-icon(icon-names, 'grey')
          padding: 10px 0
          color: #484848
          font-family 'Arial'
          font-size 16px !important
      &:hover a
      a.active
        color: var(--color-accent1) !important
        font-weight normal
        override-svg-icon(icon-names, 'lightgrey')
        @media mobile-big-and-below
          //override-svg-icon-with-default(icon-names)
          override-svg-icon(icon-names, 'grey')
          color: #0c1f8f !important
          font-weight normal
          font-family var(--font1-bold)
      a[href$="live"]
        font-family: var(--font1-bold)
        background-color: transparent
        span
          letter-spacing: 2px
          background-color: var(--color-accent2)
          color: #000
          padding: 0 12px
          padding-top:2px
          padding-bottom:2px
          font-weight: bold
      a[href*="twitter.com"]
        display: block
        width: 100%
        box-sizing: border-box
        padding-left: 38px;
        border-radius: 5px;
        background-color: #0095E7
        background-image: url('~@/assets/images/brands/generic/social/twit.svg')  !important
        background-size: 20px 20px
        background-position: 10px center
        background-repeat: no-repeat
        color: #fff
  ul.site-navigation li a.router-link-active
    @media mobile-big-and-above
      font-weight bold !important
      font-family 'Arial' !important
  .sports-menu
    .navigation-group .router-link-exact-active.router-link-active
      color: var(--color-accent1) !important
      @media mobile-big-and-below
        color: #0c1f8f !important
    .site-navigation-empty
